mixin-grid(columns)
  display: grid
  grid-template-columns: columns
  align-items: center

mixin-paragraph()
  font-family: font
  font-weight: weight_bold
  color: blue_medium

mixin-header()
  font-family: font
  font-weight: weight_bold
  color: blue_medium

mixin-flex(position= center)
  display: flex
  align-items: center
  justify-items: position